#
# no b-frames -- keep the profile simple!
# no cabac arithimetic coding. (corresponds to specs for level 1.3 video)
#
coder=0
bf=0
refs=1
level=31
b=2048000
maxrate=2250000
bufsize=4096000
wpredp=0
vprofile=baseline

